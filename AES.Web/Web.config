    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ISecuritySvc" />
                <binding name="BasicHttpBinding_IOpeningSvc" />
                <binding name="BasicHttpBinding_IApplicationSvc" />
                <binding name="BasicHttpBinding_IJobbingSvc" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:61658/SecuritySvc.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISecuritySvc" contract="SecurityService.ISecuritySvc" name="BasicHttpBinding_ISecuritySvc" />
            <endpoint address="http://localhost:7437/OpeningSvc.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IOpeningSvc" contract="OpeningService.IOpeningSvc" name="BasicHttpBinding_IOpeningSvc" />
            <endpoint address="http://localhost:55707/ApplicationSvc.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IApplicationSvc" contract="ApplicationService.IApplicationSvc" name="BasicHttpBinding_IApplicationSvc" />
            <endpoint address="http://localhost:61586/JobbingSvc.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IJobbingSvc" contract="JobbingService.IJobbingSvc" name="BasicHttpBinding_IJobbingSvc" />
        </client>
