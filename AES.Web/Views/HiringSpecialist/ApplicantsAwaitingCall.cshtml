@using AES.Web.Models
@model IEnumerable<HiringSpecialistModel>

@{
    ViewBag.Title = "ApplicantsAwaitingCall";
    Layout = "~/Views/Shared/_LayoutEmpty.cshtml";
}

<div id="ApplicantsAwaitingCall">

    <div>
        @foreach (HiringSpecialistModel AppAwaitCall in Model)
        {
            Html.RenderPartial("PartialApplicantsAwaitingCall", AppAwaitCall);
        }
    </div>

</div>

