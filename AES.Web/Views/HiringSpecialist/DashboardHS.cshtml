@using AES.Web.Models
@model IEnumerable<HiringSpecialistModel>

@{
    ViewBag.Title = "DashboardHS";
    Layout = "~/Views/HiringSpecialist/_LayoutDashHS.cshtml";
}

<div id="DashboardHS">

    <div id="Title">
        <div class="jumbotron text-center">
            <div class="container-fluid">
                <div class="col-lg-12">
                    <h1>Applicants Awaiting Interview</h1>
                </div>
            </div>
        </div>
    </div>

    <div id="body">
        <div class="container-fluid bg-diag-grad">
            @{Html.RenderAction("ApplicantsAwaitingCall", "HiringSpecialist");}
        </div>
    </div>
   
  

</div>

